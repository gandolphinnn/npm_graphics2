	"bugs": {
		"url": "https://github.com/gandolphinnn/npm_graphics2/issues"
	},
	"homepage": "https://github.com/gandolphinnn/npm_graphics2#readme",
	"dependencies": {
		"@gandolphinnn/utils": "^3.0.2"
	},
	"devDependencies": {
		"@parcel/config-default": "^2.10.3",
		"@parcel/transformer-typescript-tsc": "^2.10.3",
		"parcel-reporter-static-files-copy": "^1.5.3"
	}
}

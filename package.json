		"@gandolphinnn/utils": "^3.1.0"
	},
	"devDependencies": {
		"html-webpack-plugin": "^5.6.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.5.4",
		"webpack": "^5.93.0",
